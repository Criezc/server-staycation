<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="show-tab"
>
  <div class="row">
    <% for(let x = 0; x < item.imageId.length; x++){%>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="https://admin-staycation.herokuapp.com/<%= item.imageId[x].imageUrl%>"
            alt="imageUrl"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <%}%>
  </div>
  <a href="/admin/booking" class="btn btn-warning btn-sm">
    Back<i class="fas fa-arrow-left btn-sm"> </i>
  </a>
</div>
